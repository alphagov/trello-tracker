{
  "manifest_version": 2,
  "name": "Trello Tracker",
  "description": "Execute Trello reports for the Content Team",
  "version": "1.0",
  "icons": {
    "48": "icons/icon128.png"
  },
  "permissions": [
    "tabs",
    "https://trello.com/*"
  ],
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": false
  }
}

